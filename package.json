  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "assert": "^1.4.1",
    "cross-env": "^5.1.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-react-app": "^2.1.0",
    "expect": "^23.1.0",
    "fetch-mock": "^6.4.3",
